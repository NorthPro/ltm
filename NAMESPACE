# Exported functions
export(anova.grm, anova.ltm, anova.rasch, coef.grm, coef.ltm, coef.rasch, descript, factor.scores, factor.scores.grm,
		factor.scores.ltm, factor.scores.rasch, fitted.grm, fitted.ltm, fitted.rasch, grm, information, ltm, margins, 
